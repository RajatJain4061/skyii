{
    "name": "foqat/yati",
    "description": "Yii 2 Advanced Template Improved",
    "keywords": ["yati", "yii2", "advanced", "foqat"],
    "homepage": "http://foqat.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/foqat/yati/issues?state=open",
        "forum": "http://www.foqat.com/forum/",
        "wiki": "http://www.foqat.com/wiki/",
        "source": "https://github.com/foqat/yati"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-httpclient": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
		"vendor-dir": "./../yii-framework2/vendor"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "./../yii-framework2/vendor/npm",
            "bower-asset-library": "./../yii-framework2/vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
